body header.header.header--middle-left.page-width{
  background: aqua!important;
  color: #000!important;
}
/**Homepage Start**/
.hero-banner-main, .four-products-image, .most-popular-cards, .four-landing-pages{
  padding: 20px 42px 0px 42px;
  @media screen and (max-width: 768px){
    padding: 20px 20px 0px 20px;
  }
}

 h1.banner__heading{
   font-size: 80px;
   font-family: Gotham;
   line-height: 1.1em;
   font-weight: normal;
   font-style: normal;
   letter-spacing: normal;
   margin: 0;
   padding: 10px 0;
   @media screen and (max-width: 1440px){
     font-size: 50px;
   }
   @media screen and (max-width: 1024px){
    font-size: 40px;
   }
 }
p.banner__subheading{
  font-size: 20px;
  letter-spacing: -0.005em;
  line-height: 1.714;
  font-weight: 400;
  color: #1b4388;
  margin: 0;
  margin-top: 15px;
  @media screen and (max-width: 1440px){
     font-size: 18px;
   }
}
.hero-button{
    color: rgba(27, 67, 136, 1);
    background: #fff;
    border-radius: 50px;
    font-size: 13px;
    font-family: Gotham;
    display: inline-block;
    padding-top: 16px;
    padding-left: 65px;
    padding-bottom: 16px;
    padding-right: 65px;
    position: absolute;
    bottom: -90px;
    left: 0;
    transition: 0.2s ease-in-out;
    &:hover{
      border-color: rgba(255, 255, 255, 1) !important;
      background-color: rgba(27, 67, 136, 1) !important;
      color: rgba(255, 255, 255, 1) !important;
    }
    @media screen and (max-width: 768px){
      position: unset;
      max-width: unset;
      width: 100%;
      text-align: center;
      margin-top: 32px;
    }
    @media screen and (max-width: 767px){
      padding: 16px 0px;
    }
}
.hero-banner-main{
  .hero-banner{
    position: relative;
    width: 100%;
    @media screen and (max-width: 768px){
      display: flex;
      flex-direction: column-reverse;
      flex-wrap: wrap;
    }
    img{
      width: 100%;
      border-top-right-radius: 20px;
      border-top-left-radius: 20px;
      display: block;
      @media screen and (max-width: 768px){
        border-radius: unset;
        position: relative;
        z-index: -1;
        margin-top: -10px;
      }
       @media screen and (max-width: 767px){
        height: 30vh;
        object-fit: cover;
        object-position: right;
       }
    }
    .hero-text-box{
        background: #fff;
        position: absolute;
        top: 50%;
        transform: translateY(-50%);
        border-radius: 12px;
        max-width: 856px;
        padding: 40px;
        left: 45px;
        @media screen and (max-width: 1440px){
          max-width: 545px;
        }
       @media screen and (max-width: 1100px){
          max-width: 450px;
          top: 40%;
       }
       @media screen and (max-width: 1024px){
          top: 33%;
       }
      @media screen and (max-width: 768px){
        position: unset;
        max-width: unset;
        transform: unset;
        width: 100%;
        background-color: #edeff1;
        border-bottom-right-radius: unset;
        border-bottom-left-radius: unset;
      }
    }
    
  }
 
}
.marquee-container{
  background: rgba(27, 67, 136, 1);
  border-bottom-right-radius: 20px;
  border-bottom-left-radius: 20px;
  overflow: hidden;
  @media screen and (max-width: 1000px){
    display: none;
  }
}
.marquee {
    display:  inline-block;
    white-space: nowrap;
    position: relative;
    transform: translate3d(0%, 0, 0);
    animation-name: marquee;
    animation-timing-function: linear;
    animation-iteration-count: infinite;
	animation-duration: 50s;
	color: #fff;
	letter-spacing: 1px;
    padding-top: 28px;
    padding-bottom: 28px;
    line-height: 1.27em;
    font-family: Gotham-book;
    font-size: 15px;
}
.marquee:hover { 
  animation-play-state: paused;
}
.marquee a {
    display: inline-block;
    white-space: nowrap;
    padding-right: 0rem;
    text-transform: uppercase;
    font-family: Gotham-book;
    font-size: 15px;
    letter-spacing: 1px;
    color: #fff;
    line-height: 1.27em;
	text-decoration: none;
}

@keyframes marquee {
    0% {
        transform: translate3d(50vw, 0, 0);
    }

    100% {
        transform: translate3d(-100%, 0, 0);
    }
}

.four-products-image{
  @media screen and (max-width: 768px){
    padding: 20px 42px 0;
  }
      .main-text-block{
        padding: 60px 0;
        h2{
          margin: 0;
          font-size: 42px;
          font-family: "Gotham";
          line-height: 1.19em;
          letter-spacing: normal;
        }
        p{
          font-family: "Poppins";
          font-size: 20px;
          line-height: 1.4em;
          max-width: 780px;
        }
      }
  .left-side-main-container{
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    @media screen and (max-width: 768px){
      flex-direction: column-reverse;
    }
    img{
      width: 100%;
      display: block;
      border-radius: 10px;
      object-fit: cover!important;
    }
    .left-side-container{
      width:calc(46% - 18.5px)!important;
      position: relative;
      @media screen and (max-width: 1024px){
        width:calc(46% - 10px)!important;
      }
      @media screen and (max-width: 768px){
        width:100%!important;
      }
      .text-block{
        position: absolute;
        bottom: 0px;
        background: #fff;
        margin-left: 26px;
        margin-bottom: 28px;
        margin-right: 26px;
        padding: 35px;
        border-radius: 10px;
        @media screen and (max-width: 1024px){
          margin-left: 15px;
          margin-bottom: 15px;
          margin-right: 15px;
        }
        h3{
          margin: 0;
          font-family: "Gotham";
          font-size: 42px;
          line-height: 1em;
          letter-spacing: 0px;
          @media screen and (max-width: 1024px){
            font-size: 30px;
          }
        }
        p{
          font-size: 20px;
          line-height: 28px;
          letter-spacing: normal;
          margin: 0;
          margin-top: 15px;
          @media screen and (max-width: 1024px){
            line-height: 24px;
          }
        }
      }
      img{
        height: 100%; 
      }
    }
    .right-side-container{
      width:calc(54% - 18.5px)!important;
      display: flex;
      flex-wrap: wrap;
      justify-content: space-between;
      @media screen and (max-width: 768px){
        width:100%!important;
        padding-left: 15px;
      }
      .product-card{
        width: calc(50% - 15px);
        &:nth-child(-n + 2){
          margin-bottom: 30px;
        }
        img{
          border-bottom-left-radius: unset;
          border-bottom-right-radius: unset;
        }
        h4{
          border: 1px solid #1b4388;
          display: block;
          margin: 0;
          padding: 10px 0;
          font-size: 13px!important;
          line-height: 41.2px!important;
          letter-spacing: .78px!important;
          text-align: center;
          font-weight: bold;
          text-transform: uppercase;
          border-bottom-left-radius: 10px;
          border-bottom-right-radius: 10px;
          transition: 0.2s ease-in-out;
          &:hover{
            background: #1b4388;
            color: #fff;
          }
          @media screen and (max-width: 1024px){
            font-size: 11px!important;
            line-height: 20px!important;
          }
        }
      }
      .slick-list.draggable{
        padding-top: 50px;
        .product-card.slick-slide{
          padding-right: 20px;
        }
      }
      .slick-arrow{
          position: absolute;
          right: 30px;
          top: 28px;
          background: 0 0;
          background-repeat: no-repeat;
          border: none!important;
          font-size: 0px!important;
          z-index: 99999;
          width: 50px;
          padding: 0;
          height: 40px;
          background-color: #fff;
          background-position: center center;
          cursor: pointer;
    }
      .slick-next{
        background-image: url('right-arrow1.svg');
        right: 26px;
        border: 1px solid #1b4388!important;
        border-left: unset!important;
        border-top-right-radius: 6px;
        border-bottom-right-radius: 6px;
      }
      .slick-prev{
        background-image: url('left-arrow1.svg');
        right: 75px;
        border: 1px solid #1b4388!important;
        border-right: unset!important;
        border-top-left-radius: 6px;
        border-bottom-left-radius: 6px;
      }
      
    }
  }
}

